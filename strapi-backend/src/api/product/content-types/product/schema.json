{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "productId": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "currency": {
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "rings",
        "earrings",
        "pendants",
        "bracelets-bangles"
      ]
    },
    "sku": {
      "type": "string"
    },
    "materials": {
      "type": "json"
    },
    "weight": {
      "type": "decimal"
    },
    "dimensions": {
      "type": "string"
    },
    "sizes": {
      "type": "json"
    },
    "gemstones": {
      "type": "json"
    },
    "inStock": {
      "type": "boolean",
      "default": true
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "tags": {
      "type": "json"
    },
    "body": {
      "type": "blocks"
    },
    "originalCreatedAt": {
      "type": "date"
    },
    "originalUpdatedAt": {
      "type": "date"
    }
  }
}
